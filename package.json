{
  "name": "koliada-syntax-highlighter",
  "displayName": "Koliada Syntex Highlighter",
  "description": "Adds syntax highlighting for the Koliada programming language, making it easier to read and write Koliada code in VSCode.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": "https://github.com/yourusername/koliada-syntax-highlighter",
  "contributes": {
    "languages": [{
      "id": "koliada",
      "aliases": ["Koliada", "koliada"],
      "extensions": [".koliada",".kld"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "koliada",
      "scopeName": "source.koliada",
      "path": "./syntaxes/koliada.tmLanguage.json"
    }]
  }
}